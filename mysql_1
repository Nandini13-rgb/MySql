use college;
create table Student(id integer, name varchar(15), department varchar(15));
insert into Student values(1,"adam","CSE");
insert into Student values(2,"ewa","ECE");
insert into Student values(3,"Jack","EE");
insert into Student values(4,"Karim","CE");

select * from Student;
select * from Student where(name = "ewa") ;
select * from Student where(id = 2);
-- select * from Student where(department = "CE")

create database university;
use university;
create table Professor(id integer, department varchar(15));
insert into Professor values(1,"CSE");
insert into Professor values(2,"ECE");
insert into Professor values(3,"EE");
insert into Professor values(4,"CE");

select * from Professor;
